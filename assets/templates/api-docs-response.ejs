<h4>Response reference</h4>

<% if (this.description) { %>
  <p><%= this.description %></p>
<% } %>

<table class="tbl">
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>JSON Type</th>
      <th>Multiple</th>
    </tr>
  </thead>
  <tbody>
    <% for (var i = 0, ii = this.fields.length; i < ii; i++) { %>
      <tr>
        <td><a href="#api-docs-types-<%= this.fields[i].type.toLowerCase() %>"><%= this.fields[i].name %></a></td>
        <td><a href="#api-docs-types-<%= this.fields[i].type.toLowerCase() %>"><%= this.fields[i].type %></a></td>
        <td><a href="#api-docs-types-<%= this.fields[i].type.toLowerCase() %>"><%= this.fields[i].json_type %></a></td>
        <td><a href="#api-docs-types-<%= this.fields[i].type.toLowerCase() %>"><%= this.fields[i].is_multiple %></a></td>
      </tr>
    <% } %>
  </tbody>
</table>
